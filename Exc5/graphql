 type Query {
    client(id: ID!): Client
    documents(id: ID!): [Document]
    relatives(id: ID!): [Relative]
}

 type Client{
     id: String
     name: String
     age: int
}

 type Document{
     id: String
     type: String
     number: String
     issueDate: String
     expiryDate: String
}
 type Relative{
     id: String
     relationType: String
     name: String
     age: int
 }
 